<Window x:Class="YouViewer.CustomView.InputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Create new bookmark" Height="92" Width="300">
    <StackPanel>
        <TextBox x:Name="inputText" Height="28" AcceptsReturn="True" MaxLength="20" />
        <Button Content="OK" Height="23" Name="btnOK" Width="75" Click="btnOK_Click" IsDefault="True" />
    </StackPanel>
</Window>
